{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./lib", "./components", "./types"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "name": "Bitrobot Documentation",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "exclude": ["**/*.test.ts", "**/*.test.tsx", "**/node_modules/**"],
  "readme": "./README.md",
  "plugin": [],
  "theme": "default",
  "hideGenerator": true,
  "searchInComments": true,
  "categorizeByGroup": true,
  "sort": ["source-order"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "intentionallyNotExported": [
    "BonusBannerProps",
    "DiscoverCarouselProps",
    "EarningsCardProps",
    "EarningsHistoryProps",
    "LeaderboardProps",
    "HeaderProps",
    "MobileMenuProps",
    "SidebarProps",
    "BadgeProps",
    "ButtonProps",
    "CardProps",
    "EarnPointsBadgeProps",
    "EmptyStateProps",
    "ErrorFallbackProps",
    "ErrorBoundaryProps",
    "ErrorBoundaryState",
    "IconProps",
    "SectionHeaderProps",
    "SkeletonProps"
  ],
  "blockTags": [
    "@author",
    "@callback",
    "@category",
    "@categoryDescription",
    "@default",
    "@defaultValue",
    "@deprecated",
    "@document",
    "@enum",
    "@event",
    "@eventProperty",
    "@example",
    "@expand",
    "@extends",
    "@fileoverview",
    "@group",
    "@groupDescription",
    "@hidden",
    "@hideCategories",
    "@hideGroups",
    "@ignore",
    "@import",
    "@include",
    "@includeCode",
    "@inheritDoc",
    "@interface",
    "@internal",
    "@label",
    "@license",
    "@link",
    "@linkcode",
    "@linkplain",
    "@mergeModuleWith",
    "@module",
    "@namespace",
    "@overload",
    "@packageDocumentation",
    "@param",
    "@private",
    "@privateRemarks",
    "@property",
    "@protected",
    "@public",
    "@readonly",
    "@remarks",
    "@returns",
    "@satisfies",
    "@sealed",
    "@see",
    "@since",
    "@template",
    "@throws",
    "@type",
    "@typedef",
    "@typeParam",
    "@useDeclaredType",
    "@version",
    "@virtual"
  ]
}
